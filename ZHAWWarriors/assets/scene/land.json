		],
		[
			{ class: "land" },
			{ class: "witch_hat", x: -200 },
			{ class: "witch" }
		],
		[
			{ class: "land" }
		],
		[
			{ class: "land" },
			{ class: "witch" },
			{ class: "witch", y: -200 }
		],
		[
			{ class: "land" },
			{ class: "box", handler: trigger, event: contact, load: '' }
		]
